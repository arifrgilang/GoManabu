<?xml version = "1.0" encoding = "UTF-8"?>
<aiml>
   <category>
      <pattern> KANJI * </pattern>
      <template>
        <think><set name = "state"><star/></set></think>
        <condition name = "state" value = "bulan">
            Bulan (Kanji: 月) (Hiragana: つき)(Katakana: ツキ)(Romaji: Tsuki)
        </condition>

        <condition name = "state" value = "pohon">
            Pohon (Kanji: 木) (Hiragana: き)(Katakana: キ)(Romaji: Ki)
        </condition>
      </template>
   </category>

</aiml>
